#!/usr/bin/env bash
# üèØ AgencyOS CLI Wrapper - Convenience script for release management
# Usage: cc release <command> [args...]

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
