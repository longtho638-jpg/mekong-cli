{
  "redirects": [
    {
      "source": "/app",
      "destination": "/app/dashboard",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/docs/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/app/:path*",
      "destination": "https://dashboard-rm4rowo86-minh-longs-projects-f5c82c9b.vercel.app/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://api.agencyos.network/:path*"
    },
    {
      "source": "/en/:path*",
      "destination": "https://frontend-fjfbga9f0-minh-longs-projects-f5c82c9b.vercel.app/en/:path*"
    },
    {
      "source": "/vi/:path*",
      "destination": "https://frontend-fjfbga9f0-minh-longs-projects-f5c82c9b.vercel.app/vi/:path*"
    },
    {
      "source": "/zh/:path*",
      "destination": "https://frontend-fjfbga9f0-minh-longs-projects-f5c82c9b.vercel.app/zh/:path*"
    },
    {
      "source": "/newsletter",
      "destination": "https://newsletter-saas-one.vercel.app"
    },
    {
      "source": "/newsletter/:path*",
      "destination": "https://newsletter-saas-one.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/emails",
      "schedule": "0 * * * *"
    }
  ]
}
