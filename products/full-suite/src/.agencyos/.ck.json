{
    "codingLevel": -1,
    "privacyBlock": true,
    "docs": {
        "maxLoc": 800
    },
    "plan": {
        "namingFormat": "{date}-{issue}-{slug}",
        "dateFormat": "YYMMDD-HHmm",
        "issuePrefix": "AO-",
        "reportsDir": "reports",
        "resolution": {
            "order": [
                "session",
                "branch"
            ],
            "branchPattern": "(?:feat|fix|chore|refactor|docs)/(?:[^/]+/)?(.+)"
        },
        "validation": {
            "mode": "prompt",
            "minQuestions": 3,
            "maxQuestions": 8,
            "focusAreas": [
                "assumptions",
                "risks",
                "tradeoffs",
                "win-win-win"
            ]
        }
    },
    "paths": {
        "docs": "docs",
        "plans": "plans"
    },
    "locale": {
        "thinkingLanguage": "vi",
        "responseLanguage": "vi"
    },
    "trust": {
        "passphrase": null,
        "enabled": false
    },
    "project": {
        "type": "agency",
        "packageManager": "auto",
        "framework": "antigravity"
    },
    "assertions": []
}